# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_GRAPHQL_URL=http://localhost:8000/graphql

# Application Configuration
REACT_APP_APP_NAME=PMS
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=Project Management System

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_CALENDAR=true
REACT_APP_ENABLE_GANTT_CHART=true
REACT_APP_ENABLE_REAL_TIME=true

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-here
REACT_APP_TOKEN_EXPIRY=86400

# External Services
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false
